def fun_list_1(n):
    a=0
    for i in range(n):
        a=a+i*(i-1)
    return(a)

def fun_list_2(a,b,c):
    if a==b:
        if a==c:
            print(3)
        else:
            print(2)
    elif a==c :
            print(2)
    else:
        print(0)

